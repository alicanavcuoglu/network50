{
  "version": 2,
  "builds": [
      {"src": "app.py", "use": "@vercel/python"}
  ],
  "routes": [
      {"src": "/(.*)", "dest": "app.py"}
  ],
  "env": {
    "DATABASE_URL": "DATABASE_URL",
    "MAIL_SERVER": "MAIL_SERVER",
    "MAIL_USERNAME": "MAIL_USERNAME",
    "MAIL_PASSWORD": "MAIL_PASSWORD",
    "SECRET_KEY": "SECRET_KEY"
  }
}